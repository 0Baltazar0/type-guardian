{
  "name": "@bunnio/type-guardian",
  "version": "0.0.8-rc.4",
  "description": "Openapi translator to typescript interface and zod objects",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "zod-gen": "ts-node src/zodBuilder",
